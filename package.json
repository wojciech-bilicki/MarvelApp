{
  "name": "MarvelApp",
  "version": "0.1.0",
  "main": "index.js",
  "repository": "https://github.com/wojciech-bilicki/MarvelApp.git",
  "author": "Wojciech Bilicki <wojciech.bilicki@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prettify": "prettier --write --single-quote --print-width=90 \"js/**/*.{js,jsx}\"",
    "lint": "eslint .",
    "bundle": "webpack",
    "bundle:watch": "webpack --watch",
    "start": "webpack-dev-server",
    "test": "NODE_ENV=test jest",
    "updateSnapshots": "NODE_ENV=test jest --u",
    "testCoverage": "NODE_ENV=test jest --coverage",
    "testWatch": "NODE_ENV=test jest --watch",
    "flowCoverage": "flow-coverage-report -i 'src/**/*.js' -i 'src/**/*.jsx' -x 'src/test/**' -t html -t json -t text",
    "flow": "flow",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "prettier": "^1.5.3",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "redux": "^3.7.2",
    "styled-components": "^2.1.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.6",
    "@storybook/addon-links": "^3.2.6",
    "@storybook/react": "^3.2.8",
    "babel-cli": "^6.26.0",
    "babel-eslint": "7",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^2.9.1",
    "eslint": "3.x",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-config-prettier": "^2.3.0",
    "eslint-config-react": "^1.1.7",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.1.0",
    "file-loader": "^0.11.2",
    "flow-bin": "^0.53.1",
    "flow-coverage-report": "^0.3.0",
    "istanbul": "^0.4.5",
    "jest": "^20.0.4",
    "jest-serializer-enzyme": "^1.0.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^15.6.1",
    "storybook-router": "^0.2.9",
    "url-loader": "^0.5.9",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "snapshotSerializers": [
      "jest-serializer-enzyme"
    ]
  }
}
